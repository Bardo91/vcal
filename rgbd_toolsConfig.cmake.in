endif(@fastcom_FOUND@)


if(@catkin_FOUND@)
    find_dependency(catkin REQUIRED roscpp rosbag cv_bridge image_transport sensor_msgs)
endif(@catkin_FOUND@)

if(NOT TARGET vcal::vcal)
    include("${rgbd_tools_CMAKE_DIR}/rgbd_toolsTargets.cmake")
endif()
