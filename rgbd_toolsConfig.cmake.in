get_filename_component(rgbd_tools_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
include(CMakeFindDependencyMacro)

find_package(OpenCV @OpenCV_VERSION@ REQUIRED core highgui imgproc imgcodecs features2d xfeatures2d calib3d video videoio)

find_package(rgbd_tools)


